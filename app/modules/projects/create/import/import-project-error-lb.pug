tg-lightbox-close

.content(ng-switch="key")
    .private-space(ng-switch-when="private-space")
        img(
            src="/#{v}/svg/icons/project-limit.svg"
        )

        h2.title(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PRIVATE_PROJECTS_SPACE.TITLE"
            [translateParams]="values"
        )
        p.description(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PRIVATE_PROJECTS_SPACE.DESC"
            [translateParams]="values"
        )

    div(ng-switch-when="private-members")
        img(
            src="/#{v}/svg/icons/block-user.svg"
        )

        h2.title(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PRIVATE_PROJECTS_MEMBERS.TITLE"
            [translateParams]="values"
        )
        p.description(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PROJECT_MEMBERS_DESC"
            [translateParams]="values"
        )

    div(ng-switch-when="private-space-members")
        img(
            src="/#{v}/svg/icons/multi-block-project.svg"
        )

        h2.title(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PRIVATE_PROJECTS_SPACE_MEMBERS.TITLE"
            [translateParams]="values"
        )
        p.description(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PRIVATE_PROJECTS_SPACE_MEMBERS.DESC"
            [translateParams]="values"
        )

    div(ng-switch-when="public-space")
        img(
            src="/#{v}/svg/icons/project-limit.svg"
        )

        h2.title(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PUBLIC_PROJECTS_SPACE.TITLE"
            [translateParams]="values"
        )
        p.description(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PUBLIC_PROJECTS_SPACE.DESC"
            [translateParams]="values"
        )

    div(ng-switch-when="public-members")
        img(
            src="/#{v}/svg/icons/block-user.svg"
        )

        h2.title(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PUBLIC_PROJECTS_MEMBERS.TITLE"
            [translateParams]="values"
        )
        p.description(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PROJECT_MEMBERS_DESC"
            [translateParams]="values"
        )

    div(ng-switch-when="public-space-members")
        img(
            src="/#{v}/svg/icons/multi-block-project.svg"
        )

        h2.title(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PUBLIC_PROJECTS_SPACE_MEMBERS.TITLE"
            [translateParams]="values"
        )
        p.description(
            translate="PROJECT.IMPORT.PROJECT_RESTRICTIONS.PUBLIC_PROJECTS_SPACE_MEMBERS.DESC"
            [translateParams]="values"
        )

    div.options
        a.button-green(
            translate="COMMON.ACCEPT"
            ng-click="close()"
        )
